<h1>Sign in</h1>
<%= form_for(:session, :url =>sessions_path) do |f| %>
	<!--<= render 'shared/error_messages' %> -->

    <div class="field">
	<%=f.label :email %><br />
	<%=f.text_field :email %>
    <div>
    <div class="field">
	<%=f.label :password %><br />
	<%=f.password_field :password %>
    <div> 
    <div class="actions">
	<%=f.submit "Sign in" %>
	<div><br>
    <p>New user? <%= link_to "Sign up Now!", signup_path %></p>
<% end %>

